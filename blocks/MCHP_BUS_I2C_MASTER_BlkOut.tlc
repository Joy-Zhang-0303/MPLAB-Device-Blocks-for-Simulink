%assign y = LibBlockOutputSignal(yIdxTab[iSeq], "", "", repeat)
%<y> = I2C%<I2CRef>%<I2C_Id>_Buff8[%<I2CSequence[iSeq*SeqL+7] + repeat>];
%endforeach
%endif
%break
%endswitch
%endforeach